{
    "name":        "cdi/cdicrm",
    "description": "Zend Framework 2 Module that provides a crm module",
    "type":        "library",
    "license":     "WTFPL",
    "keywords": [
        "crm",
        "doctrine",
        "zf2",
        "module",
        "Zend Framework"
    ],
    
    "authors": [{
        "name": "Cristian Incarnato",
        "email": "cristian.cdi@gmail.com"
    }],
    
    "minimum-stability": "dev",
    
    "config": {
    	"process-timeout": 5000
    },

    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "zendframework/zendframework": "~2.2",
        "doctrine/doctrine-orm-module": ">=0.9",
         "cdi/cdidatagrid": ">=v1.0",
         "cdi/cdientity": ">=1.0"
    },
    
    "suggest" : {
        "doctrine/doctrine-orm-module": ">=0.9"
    },
    
    "autoload": {
        "psr-0": {
            "CdiCrm\\": "src/"
        },
         "classmap": [
            "./Module.php"
            ]
    }
}
